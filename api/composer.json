{
    "name": "slim/auction",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Rodomanov Roman",
            "email": "romaxa83@ukr.net"
        }
    ],
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "require": {
        "php": "^7.4",
        "ext-json": "^7.4",
        "laminas/laminas-config-aggregator": "^1.2",
        "php-di/php-di": "^6.0",
        "slim/psr7": "^1.0",
        "slim/slim": "^4.5",
        "symfony/console": "^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files" : ["helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "scripts": {
        "app": "php bin/app.php --ansi",
        "test": "phpunit --colors=always"
    }
}
